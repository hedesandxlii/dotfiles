#define i3_focused_border   #4c7899
#define i3_focused_backgr   #285577
#define i3_focused_text     #ffffff
#define i3_unfocused_border #333333
#define i3_unfocused_backgr #222222
#define i3_unfocused_text   #888888

dwm.normbgcolor: i3_unfocused_border
dwm.normfgcolor: i3_unfocused_text
dwm.selbgcolor: i3_focused_border
dwm.selfgcolor: i3_focused_text

dwm.gappx: 10
dwm.borderpx: 5
dwm.normbordercolor: i3_unfocused_border
dwm.selbordercolor: i3_focused_border

! dmenu.normbgcolor <=> dwm.normbgcolor
! dmenu.normfgcolor <=> dwm.normfgcolor
! dmenu.selbgcolor  <=> dwm.selbgcolor
! dmenu.selfgcolor  <=> dwm.selfgcolor
